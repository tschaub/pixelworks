  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tschaub/pixelworks.git"
  },
  "bugs": {
    "url": "https://github.com/tschaub/pixelworks/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "browserify": "^16.0.0",
    "chai": "^4.1.2",
    "eslint": "^4.17.0",
    "eslint-config-tschaub": "^10.0.0",
    "karma": "^2.0.0",
    "karma-browserify": "^5.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "karma-sauce-launcher": "^2.0.0",
    "mocha": "^5.0.0",
    "sinon": "^5.0.0",
    "watchify": "^3.10.0"
  },
  "eslintConfig": {
    "extends": "tschaub",
    "globals": {
      "ImageData": false
    },
    "rules": {
      "dot-notation": 0
    }
